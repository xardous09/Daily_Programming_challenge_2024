    "def count_divisors(N):\n",
    "    count = 0\n",
    "    for i in range(1, int(N**0.5) + 1):\n",
    "        if N % i == 0:  \n",
    "            count += 1  \n",
    "            if i != N // i:  \n",
    "                count += 1 \n",
    "\n",
    "    return count"
   ]
