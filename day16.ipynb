   "source": [
    "def gcd(a, b):\n",
    "    while b:\n",
    "        a, b = b, a % b\n",
    "    return a\n",
    "\n",
    "def lcm(N, M):\n",
    "    return (N * M) // gcd(N, M)"
   ]
  },
